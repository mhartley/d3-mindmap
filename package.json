{
  "name": "d3-mindmap",
  "version": "1.0.0",
  "description": "A D3.js plugin for creating mindmaps that supports drag and drop and two way interaction with JSON data.",
  
  "typings": "dist/index.d.ts",
  "scripts": {
    "demo2": "parcel examples-src/demo2/index.html",
    "demo2:build": "parcel build examples-src/demo2/index.html --dist-dir examples/demo2",
    "lint": "eslint --fix src examples-src",
    "build": "tsc"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhartley/d3-mindmap.git"
  },
  "author": "Morgan Hartley",
  "license": "MIT",
  "dependencies": {
    "d3-hierarchy": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-transition": "^3.0.1",
    "d3-zoom": "^3.0.0",
    "d3-drag": "^3.0.0"
  },
  "devDependencies": {
    "@codemirror/lang-javascript": "^6.1.4",
    "@eslint/js": "^9.14.0",
    "@types/d3-hierarchy": "^2.0.2",
    "@types/d3-scale": "^3.3.2",
    "@types/d3-selection": "^2.0.1",
    "@types/d3-transition": "^3.0.3",
    "@types/eslint__js": "^8.42.3",
    "buffer": "^5.7.1",
    "codemirror": "^6.0.1",
    "eslint": "^9.14.0",
    "parcel": "^2.8.3",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "process": "^0.11.10",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0"
  }
}
